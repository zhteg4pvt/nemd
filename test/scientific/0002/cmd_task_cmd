echo '# 0002: Ethane Gas: polymer builder on CC via; lammps simulation; density 1.214 at 300K https://www.engineeringtoolbox.com/ethane-C2H6-density-specific-weight-temperature-pressure-d_2088.html'
run_nemd ab_lmp_traj_workflow.py "*C*" -cru_num 2 -mol_num 200 -density 0.004 -seed 5678 -temp 300 -prod_ens NPT -relax_time 4 -prod_time 4 -last_pct 0.4 -task density -state_num 6 -JOBNAME ab_lmp_traj > /dev/null
grep "g/cm^3" ab_lmp_traj.log > /dev/null