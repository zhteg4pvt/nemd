echo "0007: XYZ from XTC trajectory"
INTVL=1 nemd_run lmp_traj_driver.py $NEMD_SRC/test/data/hexane_xtc/amorp_bldr_01.xtc -data_file $NEMD_SRC/test/data/hexane_xtc/amorp_bldr.data -task xyz -NAME "XYZ_of_trajectory_(ns)" -JOBNAME "XYZ_of_trajectory_(ns)_01" -CPU 1 > /dev/null
